{
  "name": "arctic-archives",
  "version": "1.0.0",
  "description": "Personal portfolio",
  "main": "index.js",
  "scripts": {
    "compile:sass": "node-sass sass/main.scss css/style.css",
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venlahelena/arctic-archives.git"
  },
  "author": "Venla Jokikokko",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/venlahelena/arctic-archives/issues"
  },
  "homepage": "https://github.com/venlahelena/arctic-archives#readme",
  "devDependencies": {
    "node-sass": "^4.13.0"
  },
  "dependencies": {
    "auto-prefixer": "0.4.2",
    "concat": "^1.0.3",
    "sass": "^1.23.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1"
  }
}
